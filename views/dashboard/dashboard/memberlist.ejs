<div class="d-flex flex-column align-items-stretch flex-shrink-0 bg-white" style="width: 35%;">
  <a href="/" class="d-flex align-items-center flex-shrink-0 p-3 link-dark text-decoration-none border-bottom">
    <svg class="bi pe-none me-2" width="30" height="24"><use xlink:href="#bootstrap"/></svg>
    <span class="fs-5 fw-semibold">Members</span>
  </a>
  <div class="list-group list-group-flush border-bottom scrollarea">
    <% data.forEach((row)=> { %>

      <a href="/dashboard/members/<%= row.username %>" class="list-group-item list-group-item-action py-3 lh-sm" aria-current="true">
        <div class="d-flex w-100 align-items-center justify-content-between">
          <input type="hidden" name="username" value="<%= row.username %>">
          <strong class="mb-1"><%= row.username %></strong>
        </div>
        <div class="col-10 mb-1 small"><p class="card-text"><strong></strong><%=  %></p><p class="font-monospace"><%=  %></p> </div>
      </a>
  <% }); %>

  </div>
</div>
<div class="b-example-divider b-example-vr"></div>